{
  "name": "codelens-ai",
  "version": "1.0.0",
  "description": "AI-powered code analysis platform with Tree-sitter AST parsing, LSP integration, and Neo4j graph database for intelligent code understanding and relationship mapping",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest -- src/extractor/__tests__/module-level-extractor",
    "test:watch": "jest --watch",
    "test:simple": "ts-node src/index.ts",
    "analyze": "ts-node src/index.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "neo4j:start": "docker-compose up -d neo4j",
    "neo4j:stop": "docker-compose stop neo4j"
  },
  "keywords": [
    "ai",
    "artificial-intelligence",
    "machine-learning",
    "code-analysis",
    "static-analysis",
    "ast-parsing",
    "tree-sitter",
    "neo4j",
    "graph-database",
    "knowledge-graph",
    "language-server-protocol",
    "code-intelligence",
    "function-extraction",
    "code-generation",
    "refactoring",
    "embeddings",
    "vector-search",
    "langchain",
    "openai",
    "code-quality",
    "dependency-analysis",
    "software-engineering",
    "devtools"
  ],
  "author": "CodeLens AI Contributors",
  "license": "MIT",
  "homepage": "https://github.com/marvikomo/code-lens-aI#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvikomo/code-lens-aI.git"
  },
  "bugs": {
    "url": "https://github.com/marvikomo/code-lens-aI/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "ai": {
    "compatible": true,
    "features": [
      "function-extraction",
      "relationship-mapping",
      "graph-queries",
      "semantic-search",
      "call-analysis",
      "dependency-tracking"
    ],
    "formats": ["neo4j", "graphql", "embeddings", "json"],
    "languages": ["javascript", "typescript"],
    "integrations": ["openai", "langchain", "supabase"]
  },
  "dependencies": {
    "@langchain/community": "^0.3.43",
    "@langchain/core": "^0.3.56",
    "@langchain/langgraph": "^0.2.73",
    "@langchain/mistralai": "^0.2.0",
    "@langchain/openai": "^0.5.10",
    "@supabase/supabase-js": "^2.49.4",
    "dotenv": "^16.5.0",
    "express-sse": "^1.0.0",
    "fs": "^0.0.1-security",
    "graphlib": "^2.1.8",
    "langchain": "^0.3.26",
    "neo4j-driver": "^5.28.1",
    "resolve": "^1.22.10",
    "resolve-import": "^2.0.0",
    "tree-sitter": "^0.22.4",
    "tree-sitter-javascript": "^0.23.1",
    "typescript-language-server": "^4.4.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/resolve": "^1.20.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "typescript": "^4.9.5"
  }
}
